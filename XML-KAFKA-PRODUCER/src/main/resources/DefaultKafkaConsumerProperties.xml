<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="bootstrap.servers">localhost:9092</entry>
    <entry key="group.id">test</entry>
    <entry key="enable.auto.commit">true</entry>
    <entry key="auto.commit.interval.ms">1000</entry>
    <entry key="session.timeout.ms">30000</entry>
    <entry key="key.deserializer">org.apache.kafka.common.serialization.StringDeserializer</entry>
    <entry key="value.deserializer">org.apache.kafka.common.serialization.StringDeserializer</entry>
</properties>